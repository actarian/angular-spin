{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"browser": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/browser",
						"index": "src/index.html",
						"main": "src/main.browser.ts",
						"tsConfig": "src/tsconfig.app.json",
						"polyfills": "src/polyfills.ts",
						"assets": [
							"src/assets",
							"src/favicon.ico"
						],
						"extractCss": true,
						"styles": [
              				"src/styles.scss"
						],
						"scripts": [
							{ "input": "./node_modules/jquery/dist/jquery.js", "lazy": false },
							{ "input": "./node_modules/@fancyapps/fancybox/dist/jquery.fancybox.js", "lazy": false },
							{ "input": "./node_modules/mapbox-gl/dist/mapbox-gl.js", "lazy": false },
							{ "input": "./node_modules/json-formatter-js/dist/json-formatter.js", "lazy": true }
						]
					},
					"configurations": {
						"server": {
							"optimization": true,
							"outputHashing": "media",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.server.ts"
                			}]
						},
						"production": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.prod.ts"
                			}]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "browser:build",
						"port": 40000,
						"proxyConfig": "proxy.conf.json"
					},
					"configurations": {
						"production": {
							"browserTarget": "browser:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "browser:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"karmaConfig": "./karma.conf.js",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"scripts": [],
						"styles": [
              				"src/styles.scss"
            			],
						"assets": [
							"src/assets",
							"src/favicon.ico"
            			]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						],
						"exclude": [
             				 "**/node_modules/**"
            			]
					}
				}
			}
		},
		"server": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:server",
					"options": {
						"outputPath": "dist/server",
						"main": "src/main.server.ts",
						"tsConfig": "src/tsconfig.server.json",
						"bundleDependencies": "none"
					},
					"configurations": {
						"production": {
							"optimization": true,
							"outputHashing": "media",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"fileReplacements": [{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.prod.ts"
                			}]
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						],
						"exclude": [
             				 "**/node_modules/**"
            			]
					}
				}
			}
		},
		"docs": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "docs",
						"index": "src/index.html",
						"main": "src/main.browser.ts",
						"tsConfig": "src/tsconfig.app.json",
						"polyfills": "src/polyfills.ts",
						"assets": [
							"src/assets",
							"src/favicon.ico"
						],
						"extractCss": true,
						"styles": [
              				"src/styles.scss"
            			],
						"scripts": [
							{ "input": "./node_modules/jquery/dist/jquery.js", "lazy": false },
							{ "input": "./node_modules/@fancyapps/fancybox/dist/jquery.fancybox.js", "lazy": false },
							{ "input": "./node_modules/mapbox-gl/dist/mapbox-gl.js", "lazy": false },
							{ "input": "./node_modules/json-formatter-js/dist/json-formatter.js", "lazy": true }
						]
					},
					"configurations": {
						"docs": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [{
								"replace": "src/environments/environment.ts",
								"with": "src/environments/environment.docs.ts"
                			}]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "docs:build",
						"port": 40000
					},
					"configurations": {
						"production": {
							"browserTarget": "docs:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "docs:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"karmaConfig": "./karma.conf.js",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"scripts": [],
						"styles": [
              				"src/styles.scss"
            			],
						"assets": [
							"src/assets",
							"src/favicon.ico"
            			]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						],
						"exclude": [
             				 "**/node_modules/**"
            			]
					}
				}
			}
		},
		"e2e": {
			"root": "",
			"sourceRoot": "",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "./protractor.conf.js",
						"devServerTarget": "browser:serve"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
              				"e2e/tsconfig.e2e.json"
            			],
						"exclude": [
              				"**/node_modules/**"
            			]
					}
				}
			}
		}
	},
	"defaultProject": "browser",
	"schematics": {
		"@schematics/angular:component": {
			"prefix": "app",
			"styleext": "scss"
		},
		"@schematics/angular:directive": {
			"prefix": "app"
		}
	}
}
